{
    "name": "mezzio/mezzio-laminasviewrenderer",
    "description": "laminas-view PhpRenderer integration for Mezzio",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mezzio",
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "homepage": "https://mezzio.dev",
    "support": {
        "docs": "https://docs.mezzio.dev/mezzio-laminasviewrenderer/",
        "issues": "https://github.com/mezzio/mezzio-laminasviewrenderer/issues",
        "source": "https://github.com/mezzio/mezzio-laminasviewrenderer",
        "rss": "https://github.com/mezzio/mezzio-laminasviewrenderer/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "laminas/laminas-filter": "^2.5",
        "laminas/laminas-i18n": "^2.5",
        "laminas/laminas-servicemanager": "^2.5",
        "laminas/laminas-view": "^2.5",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "mezzio/mezzio-router": "^1.0",
        "mezzio/mezzio-template": "^1.0.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^1.2",
        "mezzio/mezzio": "~1.0.0-dev@dev || ^1.0",
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "suggest": {
        "aura/di": "3.0.*@beta to make use of Aura.Di dependency injection container",
        "mezzio/mezzio": "^1.0 if you wish to use the ApplicationUrlDelegatorFactory",
        "mouf/pimple-interop": "^1.0 to use Pimple for dependency injection"
    },
    "autoload": {
        "psr-4": {
            "Mezzio\\LaminasView\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MezzioTest\\LaminasView\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "replace": {
        "zendframework/zend-expressive-zendviewrenderer": "self.version"
    }
}
